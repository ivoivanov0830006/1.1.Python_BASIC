number = int(input())

num1 = int(number / 100) % 10
num2 = int(number / 10) % 10
num3 = number % 10

total = 0

for number1 in range(1, num3 + 1):
    for number2 in range(1, num2 + 1):
        for number3 in range(1, num1 + 1):
            total = number1 * number2 * number3
            print(f"{number1} * {number2} * {number3} = {total};")
